};

def lt(X, Y){
    if (X == Y){
        out = {{}};
    } else {
        out = leq(X,Y);
    };
    return out;
};

def gt(X,Y){
    t = leq(X, Y);
    if (t == {}){
        t = {{}};
    }
    else{
        t = {};
    };

    return t;

};

