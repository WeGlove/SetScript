def S(X){
    return {X} | X;
};

def pred(X){
    return ||X;
};

def add(X,Y){
    res = {};
    for (i={}; i!=X; i = S(i);){
        res = S(res);
    };

    for (i={}; i!=Y; i = S(i);){
        res = S(res);
    };

    return res;
};

def multiply(X,Y){
    res = {};
    for (i={}; i!=X; i = S(i);){
        res = add(res, Y);
    };

    return res;
};

def subtract(X, Y){
    for(i={}; i!=Y; i = S(i);){
        X = ||X;
    };

    return X;
};

def divide(X,Y){
    count = zero();
    while (geq(X,Y)){
        X = subtract(X, Y);
        count = S(count);
    };

    return <count, X>;
};

def leq(X, Y){
    while ((X != Y) | (Y != {})){
        Y = pred(Y);
    };

    out = {};
    if (X != Y){
        out = {{}};
    } else {
    };

